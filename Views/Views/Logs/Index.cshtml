@model IEnumerable<string>

<div class="container mt-4">
    <h2>Log File Monitor</h2>
        <div class="card">
                <div class="card-body">
                            <ul class="list-group">
                                            @foreach (var log in Model)
                                                            {
                                                                                <li class="list-group-item">@log</li>
                                                                                                }
                                                                                                            </ul>
                                                                                                                    </div>
                                                                                                                        </div>
                                                                                                                        </div>
                                                                                                                        